<h1>index djs</h1>

<a href=""><%= link_to "create a new dj button", new_dj_path %></a>


<div id="map"
     style="width: 100%; height: 600px;"
     data-markers="<%= @markers.to_json %>"
     data-mapbox-api-key="<%= ENV['MAPBOX_API_KEY'] %>">
</div>

     


<div class="cards">
<% @djs.each do |dj| %>
<%= link_to dj do %>
 <div class="card content">
    <div class="card-content">
      <div class="card-img">
        <%= cl_image_tag(dj.image.key) %>
      </div>
      <div class="card-label">
        <%= dj.genre %>
      </div>
      <div class="card-title">
        <%= dj.name %>
      </div>
    </div>
  </div>
<% end %>
<% end %>
